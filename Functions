pragma solidity ^0.6.0;

contract Test{
    function add() public pure returns(uint){
        uint num1 = 10;
        uint num2 = 45;
        uint sum =  num1+num2;
        return sum;
    }
}

contract Test{
    function add() public pure returns(uint){
        uint num1 = 10;
        uint num2 = 16;
        uint sum = num1+num2;
        return sqrt(sum);
    }

    function sqrt(uint num) public pure returns(uint){
        num = num**2;
        return num;
    }
}
